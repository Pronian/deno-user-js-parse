            "port": 9229,
        },
        {
            "name": "Deno Save To Export File",
            "type": "node",
            "request": "launch",
            "cwd": "${workspaceFolder}",
            "runtimeExecutable": "deno",
            "runtimeArgs": ["run", "--unstable", "--inspect-brk","-A", "mod.ts", "-s", "user-js-css-v8"],
            "outputCapture": "std",
            "port": 9229,
        }
    ]
} 
